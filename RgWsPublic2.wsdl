          <xsd:element name="commerTitle" type="xsd:string" />
          <xsd:element name="legalStatusDescr" type="xsd:string" />
          <xsd:element name="firmFlagDescr" type="xsd:string" />
          <xsd:element name="deactivationFlag" type="xsd:string" />
          <xsd:element name="deactivationFlagDescr" type="xsd:string" />
          <xsd:element name="postalAddress" type="xsd:string" />
          <xsd:element name="postalAddressNo" type="xsd:string" />
          <xsd:element name="postalZipCode" type="xsd:string" />
          <xsd:element name="postalAreaDescription" type="xsd:string" />
          <xsd:element name="registDate" type="xsd:string" />
          <xsd:element name="stopDate" type="xsd:string" />
          <xsd:element name="doy" type="xsd:string" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="rgWsPublic2BranchRtUser">
        <xsd:sequence>
          <xsd:element name="branchActivityDescr" type="xsd:string" />
          <xsd:element name="branchAddress" type="xsd:string" />
          <xsd:element name="branchZipCode" type="xsd:string" />
          <xsd:element name="branchMunicipality" type="xsd:string" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="rgWsPublic2BranchRtUserArray">
        <xsd:sequence>
          <xsd:element name="item" type="tns:rgWsPublic2BranchRtUser" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="rgWsPublic2RtUser">
        <xsd:sequence>
          <xsd:element name="errorCode" type="xsd:string" />
          <xsd:element name="errorDescr" type="xsd:string" />
        </xsd:sequence>
      </xsd:complexType>
    </xsd:schema>
  </types>

  <message name="RgWsPublic2Request">
    <part name="parameters" element="tns:rgWsPublic2AfmMethod" />
  </message>

  <message name="RgWsPublic2Response">
    <part name="parameters" element="tns:rgWsPublic2AfmMethod" />
  </message>

  <portType name="RgWsPublic2PortType">
    <operation name="rgWsPublic2AfmMethod">
      <input message="tns:RgWsPublic2Request" />
      <output message="tns:RgWsPublic2Response" />
    </operation>
  </portType>

  <binding name="RgWsPublic2Binding" type="tns:RgWsPublic2PortType">
    <soap:binding style="document"
                  transport="http://schemas.xmlsoap.org/soap/http" />
    <operation name="rgWsPublic2AfmMethod">
      <soap:operation soapAction="" />
      <input>
        <soap:body use="literal" />
      </input>
      <output>
        <soap:body use="literal" />
      </output>
    </operation>
  </binding>

  <service name="RgWsPublic2Service">
    <documentation>AADE Public Registry Lookup</documentation>
    <port name="RgWsPublic2Port" binding="tns:RgWsPublic2Binding">
      <soap:address location="https://www1.aade.gr/tameiakes/mywebservice/RgWsPublic2" />
    </port>
  </service>
