<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:tns="http://rgwspublic2.ws.public2.ws.taxinet.gr/"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             name="RgWsPublic2"
             targetNamespace="http://rgwspublic2.ws.public2.ws.taxinet.gr/">

  <types>
    <xsd:schema targetNamespace="http://rgwspublic2.ws.public2.ws.taxinet.gr/"
                elementFormDefault="qualified">
      <xsd:element name="rgWsPublic2AfmMethod">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="RgWsPublic2InputRt_in" type="tns:rgWsPublic2InputRtUser" />
